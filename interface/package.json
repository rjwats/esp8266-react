    "@types/node": "^16.11.14",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "async-validator": "^4.1.1",
    "axios": "^0.27.2",
    "http-proxy-middleware": "^2.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "notistack": "^2.0.5",
    "parse-ms": "^3.0.0",
    "react": "^18.1.0",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^18.1.0",
    "react-dropzone": "^11.4.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sockette": "^2.0.6",
